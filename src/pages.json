{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "unibest",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^fg-(.*)": "@/components/fg-$1/fg-$1.vue",
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "custom": true,
    "color": "#999999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/index.png",
        "selectedIconPath": "static/tabbar/index_se.png",
        "pagePath": "pages/home/home",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/configuration.png",
        "selectedIconPath": "static/tabbar/configuration_se.png",
        "pagePath": "pages/cloud/cloud",
        "text": "云组态"
      },
      {
        "iconPath": "static/tabbar/user.png",
        "selectedIconPath": "static/tabbar/user_se.png",
        "pagePath": "pages/mien/mien",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/home/home",
      "type": "home",
      "layout": "tabbar",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/about/alova",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "Alova 请求演示"
      }
    },
    {
      "path": "pages/about/vue-query",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "Vue Query 请求演示"
      }
    },
    {
      "path": "pages/cloud/cloud",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/index/index",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "解释"
      }
    },
    {
      "path": "pages/mien/mien",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/scanCode/scanCode",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "扫码创建设备"
      }
    },
    {
      "path": "pages/survey/survey",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "设备概括"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "demo/index",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "分包页面"
          }
        }
      ]
    }
  ]
}